  <script>
      var variant = document.querySelector('div[data-option-index="0"]');
        if(variant){
          var header = variant.querySelector('.header');
          if(header && (header.innerText.toUpperCase() != 'COLOR'  || header.innerText.toUpperCase() != 'SIZE')){
            var list = variant.querySelectorAll('.swatch-items-wrapper div');
            if(list.length == 1){
              variant.style.display = 'none'
            }
          }
          
        }
        var variant = document.querySelector('div[data-option-index="1"]');
        if(variant){
          var header = variant.querySelector('.header');
