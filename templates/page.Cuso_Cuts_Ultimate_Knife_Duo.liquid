        .stockOutTag{
            background-color: #B98800;
            border-radius: 4px;
            position: absolute;
            top: 10px;
            right: 10px;
            padding: 4px 17px;
        }
        .stockOutTag p{
            font-size: 12px;
            color: #fff;
            margin-bottom: 0px;
            text-transform: uppercase;
            font-weight: 500;
        }
        .stockOutTag::after{
            content: "";
            position: absolute;
            top: 50%;
            left: -9px;
            width: 13.63px;
            height: 10.39px;
            clip-path: polygon(100% 0, 2% 50%, 100% 100%);
            transform: translate(0%, -50%);
            background-color: #B98800;
        }
        .salePrice{
            background-color: #CC2000;
            border-radius: 4px;
            position: absolute;
            top: 10px;
            right: 10px;
            padding: 4px 17px;
        }
        .salePrice p{
            font-size: 12px;
            color: #fff;
            margin-bottom: 0px;
            text-transform: uppercase;
            font-weight: 500;
        }
        .salePrice::after{
            content: "";
            position: absolute;
            top: 50%;
            left: -9px;
            width: 13.63px;
            height: 10.39px;
            clip-path: polygon(100% 0, 2% 50%, 100% 100%);
