<body>
    <style>
        *{
            margin: 0px;
            padding: 0px;
            box-sizing: border-box;
        }
    body{
      font-family: "Neuzeit Office", sans-serif;
      -webkit-font-smoothing: antialiased;
      overflow-x: hidden!important;
      }

    h1, h2, h3, h4, h5, h6{
      font-family: "Neuzeit Office Soft Rounded", sans-serif;
    }
        .container {
            max-width: 1600px;
            margin: 0 auto;
            padding: 0 15px;
        }
        .saleThemeBtn{
            font-family: "Neuzeit Office", sans-serif;
            display: inline-block;
            text-decoration: none;
            padding: 7px 47px;
            background-color: #CC2000;
            border-radius: 4px;
            font-size: 24px;
            font-weight: 400;
            text-transform: uppercase;
            color: #fff;
            -webkit-transition: all 0.3s ease-in-out;
            -moz-transition: all 0.3s ease-in-out;
            -ms-transition: all 0.3s ease-in-out;
            -o-transition: all 0.3s ease-in-out;
            transition: all 0.3s ease-in-out;
        }
        .saleThemeBtn:hover{
            background-color: #ac1c25;
            -webkit-transition: all 0.3s ease-in-out;
            -moz-transition: all 0.3s ease-in-out;
            -ms-transition: all 0.3s ease-in-out;
            -o-transition: all 0.3s ease-in-out;
            transition: all 0.3s ease-in-out;
        }
        .mainBannerBlock{
            background-color: #000000;
            padding: 288px 0px 224px;
            position: relative;
            background-image: url('https://cdn.shopify.com/s/files/1/0589/2125/5095/files/bannerBgLayer.png?v=1674564063');
            background-repeat: no-repeat;
            background-position: center;
            background-size: cover;
        }
        .bannerTxt{
            max-width: 65%;
        }
        .bannerTxt h3{
            font-size: 24px;
            color: #fff;
            font-weight: 400;
            margin-bottom: 10px;
        }
        .bannerTxt h1{
            font-size: 97px;
            color: #fff;
            font-weight: 700;
            font-style: italic;
            margin-bottom: 0px;
            text-transform: uppercase;
        }
        .bannerTxt h2{
            font-size: 97px;
            color: #CF2001;
            font-weight: 700;
            font-style: italic;
            margin-bottom: 0px;
            text-transform: uppercase;
            line-height: 79px;
            display: flex;
            align-items: flex-start;
            margin-bottom: 20px;
        }
        .bannerTxt h2 span{
            font-size: 43px;
            color: #FFFFFF;
            font-weight: 400;
            display: inline-flex;
            align-items: flex-start;
            line-height: 42px;
        }
        .bannerTxt p{
            font-size: 24px;
            color: #fff;
            font-weight: 300;
            margin-bottom: 70px;
        }
        .bannerImgBlock{
            width: 708.12px;
            height: 100%;
            position: absolute;
            bottom: 0px;
            right: 0px;
        }
        .bannerImgBlock img{
            width: 100%;
            height: 100%;
        }

        /* Clients */

        .clientsBlock{
            background-color: #F3F3F3;
            padding: 50px 0px;
        }
        .clientFlexBox{
            display: flex;
            align-items: center;
            justify-content: space-between;
            flex-wrap: wrap;
        }
        .chientItemBox{
            width: 100.56px;
            height: 46.67px;
            text-align: center;
        }
        .chientItemBox img{
            width: 100%;
            height: 100%;
            object-fit: contain;
        }

        /* Serving Block */

        .servingBlock{
            background-color: #000000;
            padding: 73px 0px 75px;
            position: relative;
            background-image: url('https://cdn.shopify.com/s/files/1/0589/2125/5095/files/bannerBgLayer.png?v=1674564063');
            background-repeat: no-repeat;
            background-position: center;
            background-size: cover;
            overflow-x: hidden;
        }
        .sectionMianTitleTxt h5{
            font-size: 36px;
            color: #CC2000;
            font-weight: 500;
            margin-bottom: 5px;
            text-transform: uppercase;
        }
        .sectionMianTitleTxt h2{
            font-size: 52px;
            color: #fff;
            font-weight: 600;
            margin-bottom: 30px;
            text-transform: uppercase;
        }
        .sectionMianTitleTxt h2 span{
            color: #CC2000;
        }
        .mainContactBlock p{
            font-size: 26px;
            color: #fff;
            margin-bottom: 26px;
            font-weight: 200;
        }
        .mainContactBlock h6{
            font-size: 26px;
            color: #fff;
            margin-bottom: 0px;
            font-weight: 500;
        }
        .sectionMianTitleTxt{
            max-width: 52%;
        }
        .mainContactBlock{
            max-height: 520px;
            overflow-y: auto;
            padding-right: 10px;
        }
        .mainContactBlock::-webkit-scrollbar {
            width: 5px;
        }
        .mainContactBlock::-webkit-scrollbar-track {
            background: #fff; 
        }
        .mainContactBlock::-webkit-scrollbar-thumb {
            background: #CC2000; 
        }
        .ctmSliderBlock{
            width: 906px;
            position: absolute;
            top: 50%;
            right: -160px;
            transform: translate(0%, -50%);
        }
        .sideItemBlock{
            width: 100%;
            height: 452px;
            border-radius: 6px;
            overflow: hidden;
            margin-bottom: 37px;
        }
        .sideItemBlock img{
            width: 100%;
            height: 100%;
            object-fit: cover;
        }
        .swiper-slide .sideItemBlock{
            transform: scale3d(0.9, 0.9, 1);
            -webkit-transition: all 0.3s ease-in-out;
            -moz-transition: all 0.3s ease-in-out;
            -ms-transition: all 0.3s ease-in-out;
            -o-transition: all 0.3s ease-in-out;
            transition: all 0.3s ease-in-out;
        }
        .swiper-slide.swiper-slide-active .sideItemBlock{
            transform: scale3d(1, 1, 1);
            -webkit-transition: all 0.3s ease-in-out;
            -moz-transition: all 0.3s ease-in-out;
            -ms-transition: all 0.3s ease-in-out;
            -o-transition: all 0.3s ease-in-out;
            transition: all 0.3s ease-in-out;
        }
        .swiper-pagination{
            bottom: 0px !important;
        }
        .swiper-pagination-bullet{
            width: 50px;
            height: 7px;
            border-radius: 0px;
            background-color: #fff;
            opacity: 0.57;
            -webkit-transition: all 0.3s ease-in-out;
            -moz-transition: all 0.3s ease-in-out;
            -ms-transition: all 0.3s ease-in-out;
            -o-transition: all 0.3s ease-in-out;
            transition: all 0.3s ease-in-out;
        }
        .swiper-pagination-bullet.swiper-pagination-bullet-active{
            opacity: 1;
            -webkit-transition: all 0.3s ease-in-out;
            -moz-transition: all 0.3s ease-in-out;
            -ms-transition: all 0.3s ease-in-out;
            -o-transition: all 0.3s ease-in-out;
            transition: all 0.3s ease-in-out;
        }
        .servingSlideBtn{
            text-align: center;
            margin-top: 50px;
            width: 100%;
        }
        .infoProductBtnBox{
            text-align: center;
            margin-top: 0px;
            width: 100%;
        }
        .infoProductBtnBox a{
            margin-left: 205px;
        }

